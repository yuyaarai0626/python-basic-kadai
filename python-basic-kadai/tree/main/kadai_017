#Humanクラス作成
class Human:
  def __init__(self, name, age):
    self.name = name
    self.age = age

  #check_adultメソッド作成
  def check_adult(self):
    if self.age >= 20:
      print(f"{self.name}は大人です。")
    else:
      print(f"{self.name}は未成年です。")
 
#リスト作成 
human1 = Human("侍太郎", 35) 
human2 = Human("侍一郎", 15)
human3 = Human("侍次郎", 25)

human1.check_adult()
